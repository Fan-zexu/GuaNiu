{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "lib": ["DOM", "ESNext"],
      "jsx": "react-jsx",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "baseUrl": ".",
      "paths": {
        "*": ["node_modules/*"]
      }
    },
    "include": ["client/**/*", "server/**/*", "server/babel.config.js"],
    "exclude": ["node_modules"]
  }